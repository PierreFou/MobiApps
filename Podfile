# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

install! 'cocoapods'

target 'MobiApps' do

  use_frameworks!

  pod 'SwiftLint'
  pod 'Alamofire', '~> 4.7'
  pod 'SDWebImage'
  pod "PromiseKit", "~> 6.11"

  # Pods for MobiApps

  target 'MobiAppsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MobiAppsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
